<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="bottomNavColor" format="color" />


    <declare-styleable name="labelWithValue">
        <attr name="label" format="string|reference" />
        <attr name="value" format="string|reference" />
        <attr name="boldValue" format="boolean" />
        <attr name="hasDivider" format="boolean" />
    </declare-styleable>


    <declare-styleable name="flexRecycler">
        <attr name="listItem" format="reference" />
        <attr name="itemCount" format="integer" />
        <attr name="emptyState" format="reference" />
        <attr name="showEmptyState" format="boolean" />

    </declare-styleable>



</resources>